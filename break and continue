
class Main {
    public static void main(String[] args) {
         System.out.println("continue statement");
for (int i =0 ;i<10;i++){
    if (i%2==0){
        continue;
    }
    System.out.println(i);
}

 System.out.println("-------------------------------------------");
  System.out.println("break statement");

    

for(int j=1;j<10;j++){
    if(j%7==0){
        break;
    }
    System.out.println(j);
}
}
}
